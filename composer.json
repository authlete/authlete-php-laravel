{
    "name": "authlete/authlete-laravel",
    "type": "library",
    "license": "Apache-2.0",
    "description": "Authlete Library for Laravel",
    "keywords": [
        "Authlete", "Laravel",
        "OAuth", "OAuth 2.0",
        "OpenID", "OpenID Connect", "OIDC",
        "API", "Security", "SSO"
    ],
    "homepage": "https://github.com/authlete/authlete-php-laravel",
    "require": {
        "authlete/authlete": ">=1.8.0",
        "laravel/framework": ">=5"
    },
    "autoload": {
        "psr-4": {
            "Authlete\\Laravel\\": "src/Laravel"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Authlete\\Laravel\\Provider\\AuthleteServiceProvider"
            ]
        }
    }
}
